{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "pbat",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_line"
          },
          {
            "type": "SYMBOL",
            "name": "_whitespace"
          }
        ]
      }
    },
    "_line": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "label"
        },
        {
          "type": "SYMBOL",
          "name": "_statement"
        }
      ]
    },
    "_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "condition"
        },
        {
          "type": "SYMBOL",
          "name": "set_statement"
        },
        {
          "type": "SYMBOL",
          "name": "message_statement"
        },
        {
          "type": "SYMBOL",
          "name": "echo_statement"
        },
        {
          "type": "SYMBOL",
          "name": "goto_statement"
        },
        {
          "type": "SYMBOL",
          "name": "_file_statement"
        },
        {
          "type": "SYMBOL",
          "name": "add_widget_statement"
        },
        {
          "type": "SYMBOL",
          "name": "exit_statement"
        },
        {
          "type": "SYMBOL",
          "name": "return_statement"
        }
      ]
    },
    "variable_literal": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "$"
        },
        {
          "type": "PATTERN",
          "value": "[a-zA-Z0-9._]+"
        },
        {
          "type": "STRING",
          "value": "$"
        }
      ]
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "#"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "PATTERN",
              "value": "[\\n\\r]"
            }
          }
        ]
      }
    },
    "condition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "if_fail"
        }
      ]
    },
    "if_fail": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "IF"
        },
        {
          "type": "STRING",
          "value": "FAIL"
        },
        {
          "type": "SYMBOL",
          "name": "_statement"
        },
        {
          "type": "SYMBOL",
          "name": "_whitespace"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_statement"
              },
              {
                "type": "SYMBOL",
                "name": "_whitespace"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "ENDIF"
        }
      ]
    },
    "_file_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "rrm_statement"
        },
        {
          "type": "SYMBOL",
          "name": "copy_statement"
        },
        {
          "type": "SYMBOL",
          "name": "redirfile_statement"
        },
        {
          "type": "SYMBOL",
          "name": "loadexec_statement"
        }
      ]
    },
    "rrm_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "RRM"
        },
        {
          "type": "SYMBOL",
          "name": "parameter"
        }
      ]
    },
    "copy_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "COPY"
        },
        {
          "type": "SYMBOL",
          "name": "parameter"
        },
        {
          "type": "SYMBOL",
          "name": "parameter"
        }
      ]
    },
    "redirfile_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "REDIRFILE"
        },
        {
          "type": "SYMBOL",
          "name": "parameter"
        },
        {
          "type": "SYMBOL",
          "name": "parameter"
        }
      ]
    },
    "loadexec_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "LOADEXEC"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "parameter"
          }
        }
      ]
    },
    "add_widget_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "ADDWIDGET"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "parameter"
          }
        }
      ]
    },
    "exit_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "EXIT"
        },
        {
          "type": "SYMBOL",
          "name": "parameter"
        }
      ]
    },
    "set_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "SET"
        },
        {
          "type": "SYMBOL",
          "name": "string_literal"
        },
        {
          "type": "SYMBOL",
          "name": "parameter"
        }
      ]
    },
    "message_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "MESSAGE"
        },
        {
          "type": "SYMBOL",
          "name": "parameter"
        }
      ]
    },
    "echo_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "ECHO"
        },
        {
          "type": "SYMBOL",
          "name": "parameter"
        }
      ]
    },
    "return_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "RETURN"
        },
        {
          "type": "SYMBOL",
          "name": "parameter"
        }
      ]
    },
    "goto_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "GOTO"
        },
        {
          "type": "SYMBOL",
          "name": "parameter"
        }
      ]
    },
    "parameter": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "variable_literal"
        },
        {
          "type": "SYMBOL",
          "name": "string_literal"
        },
        {
          "type": "SYMBOL",
          "name": "int_literal"
        }
      ]
    },
    "label": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "PATTERN",
          "value": "[A-Z_0-9]+"
        }
      ]
    },
    "string_literal": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "[^\"$]*"
              },
              {
                "type": "SYMBOL",
                "name": "variable_literal"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "int_literal": {
      "type": "PATTERN",
      "value": "-?[0-9]+"
    },
    "_whitespace": {
      "type": "REPEAT1",
      "content": {
        "type": "PATTERN",
        "value": "[\\n\\r]"
      }
    },
    "_horizontal_whitespace": {
      "type": "PATTERN",
      "value": "[ \\t]+"
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "_horizontal_whitespace"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
